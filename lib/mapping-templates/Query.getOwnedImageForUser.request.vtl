{
    "version" : "2018-05-29",
    "operation" : "Query",
    "query": {
        "expression": "PK = :pk AND SK = :sk",
        "expressionValues": {
            ":pk": $util.dynamodb.toDynamoDBJson("USER#$ctx.args.userId"),
            ":sk": $util.dynamodb.toDynamoDBJson("OWNEDIMAGE#$ctx.args.ownedImageId")
        }
    }
}